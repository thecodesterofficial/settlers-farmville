
public class Hex {
	public enum HexType{
		LUMBER, SHEEP, BRICK, STONE, WHEAT, SAND;
	}
	
	private int x;
	private int y;
	
	
	HexType type;
	public Hex(HexType type)
	{
		this.type = type;
	}
	public void SetX(int x)
	{
		this.x = x;
	}
	public void SetY(int y)
	{
		this.y = y;
	}
	public int GetX()
	{
		return x;
	}
	public int GetY()
	{
		return y;
	}
}
